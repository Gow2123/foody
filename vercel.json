{
  "rewrites": [
    {
      "source": "/admin",
      "destination": "https://foody-admin0.vercel.app/"
    },
    {
      "source": "/admin/(.*)",
      "destination": "https://foody-admin0.vercel.app/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
} 