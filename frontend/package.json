{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "node -e \"try { require('fs').writeFileSync('./empty-module.js', 'module.exports = {};'); } catch (e) { console.error(e); }\"",
    "dev": "vite",
    "prebuild": "node ./rollup-fix.js && node ./commonjs-compat.cjs",
    "build": "ROLLUP_SKIP_NODE_RESOLVE=true ROLLUP_NATIVE_MODULES=false NODE_OPTIONS=--no-experimental-fetch vite build",
    "vercel-build": "npm run prebuild && npm run build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bootstrap": "^5.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "rollup": "^3.29.4",
    "vite": "^5.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "3.29.4",
    "@rollup/rollup-linux-x64-musl": "3.29.4"
  }
}
